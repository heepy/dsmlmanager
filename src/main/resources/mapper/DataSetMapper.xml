<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.horan.dsmlmanager.dao.DataSetMapper">
    <resultMap id="dataSetResultMap"  type="com.horan.dsmlmanager.entity.DataSet" >
        <id property="id" column="id"></id>
        <result property="dataSetName" column="dataset_name"></result>
        <result property="createTime" column="create_time"></result>
        <result property="imageTotal" column="image_total"></result>
        <result property="annotationTotal" column="annotation_total"></result>
        <result property="status" column="status"></result>
        <result property="proId" column="proId"></result>
    </resultMap>
    <select id="getPageDataSet" parameterType="map" resultMap="dataSetResultMap">
        select * from data_set
        limit #{currIndex} , #{pageSize}
        where proId=#{proId}
    </select>
    <insert id="addDataSet"  parameterType="com.horan.dsmlmanager.entity.DataSet" useGeneratedKeys="true" keyProperty="id" >
      insert into data_set(id,dataset_name,create_time,image_total,annotation_total,status,proId)
      values
       (#{id},#{dataSetName},#{createTime},#{imageTotal},#{annotationTotal},#{status},#{proId})
    </insert>
    <select id="getCount" resultType="int">
        select count(*) from data_set
    </select>
    <delete id="deleteDataSetById" parameterType="int">
        delete  from data_set where id=#{id}
    </delete>
</mapper>