<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.horan.dsmlmanager.dao.ProjectMapper">
    <resultMap id="projectResultMap"  type="com.horan.dsmlmanager.entity.Project" >
        <id property="id" column="id"></id>
        <result property="proName" column="proName"></result>
        <result property="userId" column="userId"></result>
        <result property="proDec" column="proDec"></result>
        <result property="proTime" column="proTime"></result>
        <result property="proModel" column="proModel"></result>
    </resultMap>
    <select id="getPageProject" parameterType="map" resultMap="projectResultMap">
        select * from project limit #{currIndex} , #{pageSize}
    </select>
</mapper>