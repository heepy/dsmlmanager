<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.horan.dsmlmanager.dao.ProjectMapper">
    <resultMap id="projectResultMap"  type="com.horan.dsmlmanager.entity.Project" >
        <id property="projectID" column="projectID"></id>
        <result property="projectName" column="projectName"></result>
        <result property="projectDec" column="projectDec"></result>
        <result property="projectTime" column="projectTime"></result>
        <result property="projectModel" column="projectModel"></result>
    </resultMap>
    <select id="getPageProject" parameterType="map" resultMap="projectResultMap">
        select * from project limit #{currIndex} , #{pageSize}
    </select>
</mapper>